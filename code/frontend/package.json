{
  "name": "@temphia/temphia",
  "scripts": {
    "build:portal": "ENTRY_FILE=portal rollup -c",
    "build:auth": "ENTRY_FILE=auth rollup -c",
    "build:operator": "ENTRY_FILE=operator rollup -c",
    "build:@all": "npm run build:operator && npm run build:portal && npm run build:auth && npm run build:executor:@all && npm run build:adapter_editor:@all",
    "build_ui_sdk_package": "cp -f scripts/build_package/package.json package.json && cp -f scripts/build_package/readme.md readme.md && tsc -p tsconfig_package.json",
    "###": "#####",
    "build:executor:@iframe_bootloader": "node scripts/executor/build_iframe_bootloader.js",
    "build:executor:@suborigin_bootloader": "node scripts/executor/build_suborigin_bootloader.js",
    "build:executor:pageform": "ENTRY_FILE=executor/pageform OUT_FILE=executor_pageform rollup -c",
    "build:executor:pagedash": "ENTRY_FILE=executor/pagedash OUT_FILE=executor_pagedash rollup -c",
    "build:executor:pagequery": "ENTRY_FILE=executor/pagequery OUT_FILE=executor_pagequery rollup -c",
    "build:executor:@all": "npm run build:executor:@iframe_bootloader && npm run build:executor:@suborigin_bootloader && npm run build:executor:pageform && npm run build:executor:pagedash && npm run build:executor:pagequery",
    "####": "#####",
    "build:adapter_editor:@bootloader": "node scripts/adapter_editor/build_bootloader.js",
    "build:adapter_editor:noop": "ENTRY_FILE=adapter_editor/noop OUT_FILE=adapter_editor_noop rollup -c",
    "build:adapter_editor:easypage": "ENTRY_FILE=adapter_editor/easypage OUT_FILE=adapter_editor_easypage rollup -c",
    "build:adapter_editor:@all": "npm run build:adapter_editor:@bootloader && npm run build:adapter_editor:noop && npm run build:adapter_editor:easypage",
    "#####": "#####",
    "copy:build_files": "node scripts/copy_build_files.js",
    "dev": "rollup -c -w",
    "start": "sirv public",
    "validate": "svelte-check"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "@tsconfig/svelte": "^1.0.0",
    "autoprefixer": "^10.1.0",
    "postcss": "^8.2.2",
    "postcss-load-config": "^3.0.0",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.44.2",
    "svelte-check": "^1.0.0",
    "svelte-hash-router": "^1.0.1",
    "svelte-preprocess": "^4.0.0",
    "tailwindcss": "^3.0.1",
    "tslib": "^2.0.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@krowten/svelte-heroicons": "^0.0.3",
    "@tiptap/core": "*",
    "@tiptap/starter-kit": "*",
    "@vercel/ncc": "^0.36.1",
    "autoprefixer": "^10.3.3",
    "axios": "^0.21.1",
    "chart.js": "^2.9.4",
    "codemirror": "^5.62.3",
    "dompurify": "^2.3.6",
    "flatpickr": "^4.6.9",
    "fs-extra": "^11.1.0",
    "grapesjs": "^0.20.1",
    "grapesjs-blocks-basic": "^1.0.1",
    "grapesjs-blocks-flexbox": "^0.1.1",
    "grapesjs-custom-code": "^1.0.1",
    "grapesjs-navbar": "^1.0.1",
    "grapesjs-plugin-forms": "^2.0.5",
    "grapesjs-preset-webpage": "^1.0.2",
    "grapesjs-style-filter": "^1.0.1",
    "grapesjs-style-gradient": "^2.0.14",
    "grapesjs-tabs": "^1.0.6",
    "grapesjs-tailwind": "^1.0.8",
    "grapesjs-tooltip": "^0.1.7",
    "leaflet": "^1.7.1",
    "postcss": "^8.3.6",
    "rollup-plugin-json": "^4.0.0",
    "sirv-cli": "^1.0.14",
    "svelte-emoji-selector": "^1.0.1",
    "svelte-flatpickr": "^3.2.6",
    "svelte-leafletjs": "^0.5.0",
    "svelte-markdown": "^0.2.3",
    "svelte-progress-bar": "^2.0.0",
    "svelte-routing": "^1.5.0",
    "svelte-simple-modal": "^0.6.1",
    "svelte-split-pane": "^0.1.2",
    "svelte-tooltip": "^1.2.0",
    "yootils": "0.0.17"
  }
}
