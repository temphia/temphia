{
  "objects": {
    "mainapp": {
      "type": "agent"
    },
    "maindb": {
      "type": "dyndb"
    }
  },
  "steps": [
    {
      "name": "add_main_agent",
      "type": "new_agent",
      "data": {
        "id": "default",
        "executor": "none",
        "renderer": "std.v1",
        "web_options": {
          "inject_auth": "false"
        },
        "web_files": {
          "client.js": "www/client.js",
          "client.css": "www/client.css",
          "loader": "std.loader"
        }
      }
    }, 
    {
      "name": "add_main_dydb",
      "type": "run_data_migration",
      "file": "db/schema.json"
    }
  ]
}
